{
  "name": "dhl/module-versenden-m1",
  "license": "OSL-3.0",
  "type": "magento-module",
  "description": "DHL business customer shipping extension for Magento",
  "require": {
    "php": "^8.1",
    "magento-hackathon/magento-composer-installer": "^4.0",
    "dhl/sdk-api-parcel-de": "^1.3",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "ecomdev/ecomdev_phpunit": "dev-master",
    "squizlabs/php_codesniffer": "^3.0",
    "magento-ecg/coding-standard": "^4.0",
    "phpcompatibility/php-compatibility": "^9.0",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/extension-installer": "^1.4",
    "macopedia/phpstan-magento1": "^1.1"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "magento-hackathon/magento-composer-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "phpstan": "cd ../../.. && vendor/bin/phpstan analyze -c vendor/dhl/module-versenden-m1/phpstan.neon",
    "phpstan:baseline": "cd ../../.. && vendor/bin/phpstan analyze -c vendor/dhl/module-versenden-m1/phpstan.neon -b vendor/dhl/module-versenden-m1/phpstan-baseline.neon"
  },
  "support": {
    "wiki": "http://dhl.support.netresearch.de/",
    "issues": "http://dhl.support.netresearch.de/support/tickets/new",
    "docs": "http://dhl.support.netresearch.de/support/solutions/articles/12000023174",
    "email": "dhl.support@netresearch.de"
  },
  "authors": [
    {
      "email": "plugins@netresearch.de",
      "name": "Netresearch DTT GmbH",
      "role": "Manufacturer",
      "homepage": "https://www.netresearch.de/"
    }
  ],
  "extra": {
    "magento-root-dir": "../../../html",
    "magento-deploystrategy": "symlink"
  }
}
