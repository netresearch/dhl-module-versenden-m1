                <dhl_versenden>
                    <file>dhl_versenden.xml</file>
                </dhl_versenden>
            </updates>
        </layout>
        <translate>
            <modules>
                <Dhl_Versenden>
                    <files>
                        <default>Dhl_Versenden.csv</default>
                    </files>
                </Dhl_Versenden>
            </modules>
